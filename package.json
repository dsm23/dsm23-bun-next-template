{
  "name": "dsm23-bun-next-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "fmt": "prettier . --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "fmt.check": "prettier . --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:md": "eslint \"**/*.md?(x)\" --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy=content --cache-location=.eslintmdcache",
    "lint:css": "stylelint \"**/*.css\" --max-warnings 0 --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint": "run-p lint:js lint:css",
    "check-types": "next typegen && ./node_modules/typescript/bin/tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:smoke": "vitest"
  },
